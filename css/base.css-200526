}


/* menu
---------------------------------------------------- */
.h2_bg.menu{
  background: url("../images/h2_menu_bg.jpg") center center / cover no-repeat;
}
.textbox01{
  line-height: 1.8;
}
.textbox01 .txt1{
  font-family: "游明朝", YuMincho, "ヒラギノ明朝 ProN W3", "Hiragino Mincho ProN", "HG明朝E", "MSP明朝", "MS明朝", serif;
  font-size: 2rem;
  font-weight: bold;
  margin-bottom: 20px;
}
.ul01{
 line-height: 2;
 float: left;
 margin-right: 20px;
}
.ul01 li:before{
	content: "◇";
	margin-right:10px;
}
.table01{
 border: 1px solid #ccc;
 width: 100%;
}
.table01 th{
 border: 1px solid #ccc;
 background: #eee;
 text-align: center;
}
.table01 td{
 border: 1px solid #ccc;
 vertical-align: middle!important;
}
.table01 td p{
 padding: 5px;
 font-size: 1.4rem;
}

.planbox{
 border: 1px solid #ccc;
 padding: 15px 0px;
 margin-bottom: 1em;
}
.planbox ul{ 
 padding:0px 15px;
 line-height: 2;
 list-style: circle;
 margin-left: 20px;
}
.planbox ul li p{
 float: right;
}
.nominal_cost{
 float: right;
 padding: 15px;
 background: #eee;
 font-weight: bold;
 clear: both;
}
.nominal_cost span{
 font-size: 1.2rem;
}
.set_cost{
 padding: 15px;
 text-align: center;
 background: #fff074;
 font-size: 2.6rem;
 color: crimson;
}
.set_cost p{
 display: inline-block;
 padding: 5px 10px;
 font-weight: bold;
}
.set_cost p span{
 display: inline-block;
 font-weight: bold;
 margin-right: 5px;
 font-size: 1.8rem;
}
/* 画面サイズが993px以上の場合に適用 */
@media only screen and (min-width: 993px) {
  .textbox01{
    text-align: center;
  }
  .textbox01 .txt1{
    font-size: 3rem;
  }
  .planbox ul{
   width: 47%;
   float: left;
  }
}
/* 画面サイズが992px以下の場合に適用 */
@media only screen and (max-width: 992px) {
 .planbox ul li{
  border-bottom: 1px dashed #ccc;
  margin-bottom: 10px;
 }
 .planbox ul li p{
  display: inline-block;
  text-align: right;
 }
}
/* 画面サイズが767px以下の場合に適用 */
@media only screen and (max-width: 767px) {
 .planbox ul li p{
  width: 100%;
  float: inherit;
 }
}


/* store
---------------------------------------------------- */
.h2_bg.store{
  background: url("../images/h2_store_bg.jpg") center center / cover no-repeat;
}
.store_img li{
 width: 33.3%;
 padding: 5px;
 float: left;
}
.accessbox dl{
 float: left;
 width: 50%;
 padding: 10px;
}
.accessbox dt{
 background: #eee;
 padding: 10px;
 width: 100%;
 text-align: center;
}
.accessbox dd{
 padding: 15px;
}
/* 画面サイズが767px以下の場合に適用 */
@media only screen and (max-width: 767px) {
 .store_img li{
  width: 100%;
  float: inherit;
 }
 .accessbox dl{
  width: 100%;
  float: inherit;
 }
}

/* contact
---------------------------------------------------- */
.h2_bg.contact{
  background: url("../images/h2_contact_bg.jpg") center center / cover no-repeat;
}

.contact_txtbox01{
 line-height: 2;
 margin-bottom: 20px;
}
.contact_nmbox{margin-bottom: 2em;}
.contact_nmbox > div{
	padding: 15px;
	text-align: center;
	margin-bottom: 30px;
}
.contact_nmbox .tel span,.contact_nmbox .fax{
	display: block;
	font-weight: bold;
	font-size: 3rem;
	margin-bottom: 10px;
}
.contact_nmbox dl{display: inline-block;}
span.hissu{
	line-height: 18px;
	color: #fff;
	font-size: 1.2rem;
	background: #ff4f4f;
	padding: 2px 15px;
	margin: 0px 5px;
	float: right;
}
span.ninni{
	line-height: 18px;
	color: #fff;
	font-size: 1.2rem;
	background: #222;
	padding: 2px 15px;
	margin: 0px 5px;
	float: right;
}
.contact_dl{
	border-top:1px solid #ccc;
	border-right:1px solid #ccc;
	margin-bottom: 2em;
	display:-webkit-flex; /* safari */
	display:flex;
	display: -ms-flexbox;/* IE10用 */
	-webkit-justify-content: flex-start;
	justify-content: flex-start;
	-ms-flex-pack:start;/* IE10用 */
	-webkit-flex-wrap: wrap;
	flex-wrap: wrap;
	-ms-flex-wrap: wrap;/* IE10用 */
}
.contact_dl dt{
	padding: 8px;
	border-left: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
	background: #eee;
}
.contact_dl dd{
	padding: 8px;
	border-left: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
	background: #fff;
}
.contact_dl dd{line-height: 1.6;}
.radio_dd label{margin-right: 10px;}
.form100{width: 100%;}
.needinput{background-color:#fee;border:1px solid #ccc}

input[type="text"], select{
	border:1px solid #ccc;
	padding:2px;
	line-height: 1!important;
}
textarea{font-size:100%;border:1px solid #ccc;padding:2px;}

input[type="submit"],input[type="button"],button{	
	padding: 15px 40px;
	background: #000;
	border: none;
	color: #fff;
	font-size: 1.8rem;
 -webkit-transition: all 0.3s ease-in-out;
 -o-transition: all 0.3s ease-in-out;
 transition: all 0.3s ease-in-out;
}
button:hover{
	background: #e60012;
}
input[type="radio"],input[type="checkbox"]{
	position:relative;
	top:2px;
	margin-right: 5px;
}
.kakunin{
	text-align: center;
}
.douui{
	padding: 30px;
	margin-bottom: 15px;
	text-align: center;
	border-top:1px dotted #ccc;
	border-bottom: 1px dotted #ccc;
}
.formBtn{
	text-align: center;
	margin-top: 20px;
}
.formBtn button{
	padding: 15px 40px;
	font-size: 1.8rem;
	margin: 5px;
}
.privacy{
	height: 300px;
	overflow: auto;
	margin-bottom: 1em;
}
.privacy .txtbox{
	line-height: 1.8;
	margin-bottom: 20px;
}
.privacy_dl{
	line-height:1.6;
}
.privacy_dl dt{
	font-weight:bold;
}
.privacy_dl dd{
	padding:15px;
}
.privacy_dl dd ol li{
	list-style: decimal;
	margin-left:20px;
}
/*-----------画面サイズが993px以上の時-----------*/
@media screen and (min-width: 993px) {
	.contact_nmbox > div:first-child{border-right:1px solid #ccc;} 
	.contact_nmbox dt{float: left;}
	.contact_nmbox dd{float: left; margin-right: 10px;}
	.contact_dl dt{width: 30%;}
	.contact_dl dd{width: 70%;}
}
/* 画面サイズが992px以下の場合に適用 */
@media only screen and (max-width: 992px) {
	.contact_nmbox > div:first-child{border-bottom:1px dotted #ccc;}
 .contact_nmbox > div{
  padding: 20px 0px 0px 0px;
  background: #eee;
  margin-bottom: 0px;
 }
 .contact_nmbox .tel span, .contact_nmbox .fax{
  font-size: 2.4rem;
 }
	.contact_dl dt{width: 100%;}
	.contact_dl dd{width: 100%;}
}